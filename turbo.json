{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "cuke": {
      "dependsOn": ["build"],
      "cache": false
    },
    "doc": {
      "dependsOn": ["build"],
      "cache": false
    },
    "fix": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "test": {
      "dependsOn": ["build", "unit", "cuke", "doc"],
      "cache": false
    },
    "unit": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}
