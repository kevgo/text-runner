{
  "name": "shared-cucumber-steps",
  "version": "0.0.0",
  "private": true,
  "license": "ISC",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig-build.json",
    "clean": "rm -rf dist",
    "fix": "eslint . --fix && prettier --write .",
    "lint": "prettier -l . && eslint --config=../../eslint.config.mjs --ignore-pattern=dist . && depcheck --config=../../.depcheckrc",
    "unit": "env NODE_NO_WARNINGS=1 mocha --reporter=dot 'src/**/*.test.ts'"
  },
  "devDependencies": {
    "@types/ps-tree": "1.1.6",
    "array-flatten": "3.0.0",
    "assert-no-diff": "3.0.6",
    "end-child-processes": "1.0.3",
    "globby": "14.1.0",
    "observable-process": "8.0.0-pre4",
    "ps-tree": "1.2.0",
    "strip-ansi": "7.1.0",
    "text-runner-core": "6.0.0"
  }
}
